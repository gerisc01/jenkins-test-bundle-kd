{
  "name": "request-ce-bundle-react-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate bundle for development with React and Kinetic Core",
  "scripts": {
    "start": "webpack-dashboard -c magenta -- webpack-dev-server",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack -p",
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "test:ci": "jest --ci --testResultsProcessor=./node_modules/jest-junit-reporter"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.9.0",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^20.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.11.1",
    "jest": "^20.0.1",
    "jest-junit-reporter": "^1.1.0",
    "node-sass": "^4.5.2",
    "optional-require": "^1.0.0",
    "react-test-renderer": "^15.5.4",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.0.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "bootstrap-3-card": "^0.2.0",
    "bootstrap-sass": "^3.3.7",
    "connected-react-router": "^4.2.1",
    "history": "^4.6.3",
